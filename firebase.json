{
  "hosting": {
    "source": "frontend",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1"
    }
  },
  "functions": [
    {
      "source": "backend",
      "codebase": "default",
      "ignore": [
        "**/node_modules",
        "**/.git",
        "**/firebase-debug.log",
        "**/firebase-debug.*.log",
        "**/.local"
      ]
    }
  ]
}
